n 에 길이를 넣어주고\n","# self.x_data에 x 데이터를\n","# self.y_data에 y 데이터를 넣어주고\n","# 인덱스에 따라 i 번째 데이터를 잘 리턴해주면 됩니다.\n","\n","  def __init__(self, filename, index):\n","    # TODO\n","    # 1. initialize file paths or a list of file names\n","    # 데이터셋의 전처리를 해주는 부분\n","\n","    # data = pd.read_csv('drive/My Drive/2020winter-MLcamp/content/HCV-Egy-Data.csv')\n","    data = pd.read_csv(filename, sep=',')\n","    data = data.to_numpy()\n","    \n","    self.x_data=data[:, :47236]\n","    self.y_data = data[:, 47236+index]\n","    \n","    self.length = len(data)\n","\n","  def __getitem__(self, index):\n","    return self.x_data[index], self.y_data[index]\n","\n","  def __len__(self):\n","    return self.length\n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"-LjIodPkhu6H","colab_type":"code","colab":{}},"source":["train_data = CustomDataset('drive/My Drive/datasets/rcv1subset1-train.csv', 98)\n","test_data = CustomDataset('drive/My Drive/datasets/rcv1subset2-test.csv', 98)\n","\n","X_tr = train_data.x_data\n","y_tr = train_data.y_data\n","\n","X_ts = test_data.x_data\n","y_ts = test_data.y_data"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"d91epUYak7bD","colab_type":"code","colab":{}},"source":["# Instantiate a model object\n","clf = RandomForestClassifier()\n","\n","# Set a search range\n","parameters = {'n_estimators': [200,300,400,500,1000],\n","              'criterion': ['gini']}\n","# Find the best hyperparameters using GridSearchCV\n","gridsearch = GridSearchCV(clf, parameters, scoring = 'accuracy', cv = 5)\n","gridsearch.fit(X_tr, y_tr)\n","\n","# Show the best hyperparameters\n","print(f'gridsearch.best_params_ = {gridsearch.best_params_}')\n","\n","# The best model is stored in 'best_clf'\n","best_clf = gridsearch.best_estimator_\n","best_clf"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"b2HiblQUk8Ea","colab_type":"code","colab":{}},"source":["y_pred = best_clf.predict(X_ts)\n","test_