"  def __init__(self, filename, index):\n","    # 1. initialize file paths or a list of file names\n","    # 데이터셋의 전처리를 해주는 부분\n","\n","    data = pd.read_csv(filename, sep=',')\n","    data = data.to_numpy()\n","    \n","    self.x_data=data[:, :47236]\n","    self.y_data = data[:, 47236+index]\n","    \n","    self.length = len(data)\n","\n","  def __getitem__(self, index):\n","    return self.x_data[index], self.y_data[index]\n","\n","  def __len__(self):\n","    return self.length"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"DlgLdQKQXAua","colab_type":"code","colab":{}},"source":["train_data = CustomDataset('drive/My Drive/rcv1subset1/rcv1subset1-train.csv', 32)\n","test_data = CustomDataset('drive/My Drive/rcv1subset1/rcv1subset2-test.csv', 32)\n","\n","X_tr = train_data.x_data\n","y_tr = train_data.y_data\n","\n","X_ts = test_data.x_data\n","y_ts = test_data.y_data"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"mXzYfr27Xmel","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":52},"outputId":"82496e0e-5809-49f4-c098-00862117dfcc","executionInfo":{"status":"ok","timestamp":1579666950600,"user_tz":-540,"elapsed":1743364,"user":{"displayName":"박성환학부생","photoUrl":"","userId":"02347139815461555404"}}},"source":["clf = xgb.XGBClassifier()\n","\n","parameters = {'n_estimators': [10], 'max_depth': [10, 20], 'alpha': [5]}\n","\n","gridsearch = GridSearchCV(clf, parameters, scoring = 'accuracy', cv = 5)\n","gridsearch.fit(X_tr, y_tr)\n","\n","print(f'gridsearch.best_params_ = {gridsearch.best_params_}')\n","\n","best_clf = gridsearch.best_estimator_\n","best_clf\n","\n","y_pred = best_clf.predict(X_ts)\n","test_acc = accuracy_score(y_ts, y_pred)\n","print(f'test_acc = {test_acc}')\n","\n","# y_pred = clf.predict(X_ts)\n","# prec = float(np.sum(y_pred == y_ts)) / len(y_ts)\n","# print(f'test_acc = {prec}')"],"execution_count":5,"outputs":[{"output_type":"stream","text":["gridsearch.best_params_